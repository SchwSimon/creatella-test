{
  "name": "creatella-test",
  "private": true,
  "version": "0.0.1",
  "description": "Build a grid of product items and ads",
  "main": "server/index.js",
  "scripts": {
    "start": "json-server server/index.js --routes server/routes.json --middlewares server/handle-delay.js server/handle-ads.js",
    "build": "gulp build",
    "test": "jest --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill"
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/jest-css-modules"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "author": "",
  "license": "",
  "dependencies": {
    "cool-ascii-faces": "^1.3.4",
    "json-server": "^0.9.6",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-infinite-scroller": "^1.1.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
