    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "pump": "^1.0.3",
    "sinon": "^4.2.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.6.2",
    "webpack-stream": "^4.0.0"
